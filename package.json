{
  "name": "angular-lazy-render",
  "version": "0.1.2",
  "description": "angular lazy render page",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run build:lib && ng build --app packages",
    "build:lib": "rimraf dist && ng-packagr -p lib/package.json",
    "docs": "compodoc -p tsconfig.json -n angular-lazy-render -d docs --hideGenerator",
    "docs:serve": "npm run docs -- -s",
    "prerelease": "npm run build:lib",
    "release": "standard-version",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jdb-fe/angular-lazy-render.git"
  },
  "keywords": [
    "angular",
    "angular4",
    "angular5",
    "lazy"
  ],
  "author": "bukas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdb-fe/angular-lazy-render/issues"
  },
  "homepage": "https://github.com/jdb-fe/angular-lazy-render#readme",
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cli": "~1.7.3",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.6.0",
    "codelyzer": "^4.2.1",
    "core-js": "^2.5.3",
    "husky": "^0.14.3",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "1.7.0",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.7",
    "standard-version": "^4.3.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.27.2",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.20",
    "@compodoc/compodoc": "^1.0.9"
  }
}
